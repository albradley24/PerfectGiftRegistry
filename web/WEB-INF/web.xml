<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginCtrl</servlet-name>
        <servlet-class>Support.LoginCtrl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>errorChecking</servlet-name>
        <servlet-class>MyServlets.errorChecking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EventMgrCtrl</servlet-name>
        <servlet-class>Application.EventMgrCtrl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GiftManager</servlet-name>
        <servlet-class>Gift.GiftManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileReader</servlet-name>
        <servlet-class>Gift.FileReader</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginCtrl</servlet-name>
        <url-pattern>/LoginCtrl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>errorChecking</servlet-name>
        <url-pattern>/errorChecking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventMgrCtrl</servlet-name>
        <url-pattern>/EventMgrCtrl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GiftManager</servlet-name>
        <url-pattern>/GiftManager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileReader</servlet-name>
        <url-pattern>/FileReader</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
